<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="AuthenticationFailure">
    <DisplayName>AuthenticationFailure</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload>
                <url-open xmlns:dp="http://www.datapower.com/schemas/management" xmlns:env="http://schemas.xmlsoap.org/soap/envelope/">
                    <responsecode>401</responsecode>
                    <content-type>text/xml</content-type>
                    <headers>
                        <header name="X-Backside-Transport">FAIL FAIL</header>
                        <header name="Content-Type">text/xml</header>
                        <header name="Date">Wed, 20 Dec 2017 06:23:05 GMT</header>
                    </headers>
                    <response>
                        <aaa>Failed</aaa>
                    </response>
                </url-open>
            </Payload>
            <StatusCode>401</StatusCode>
            <ReasonPhrase>Unauthorized</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>